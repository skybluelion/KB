<script>
  $(function() {
    $('#eachBtn').click(function() {
		//버튼 클릭시 배열 생성
		var array = [];
		array.push("문동은");
		array.push("박연진");
		array.push("이사라");
		array.push("손명호");
		
/* 		alert(aaray.toString()); */
 		alert(array.length); 
    
    });
  });
</script>
